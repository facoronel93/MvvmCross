      android:layout_height="wrap_content"
      local:MvxBind="Text Usuario" />
  <TextView
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="ContraseÃ±a" />
  <EditText
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      local:MvxBind="Text Password" />
  <Button
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Ingresar"
